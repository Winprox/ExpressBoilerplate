generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("DB_URL")
  shadowDatabaseUrl = env("SHADOW_DB_URL")
}

model User {
  id      String   @id @default(uuid())
  name    String
  pass    String
  ifAdmin Boolean  @default(false)
  session Session?
}

model Session {
  id       String @id
  token    String
  issuedTo String
  user     User   @relation(fields: [id], references: [id])
}
