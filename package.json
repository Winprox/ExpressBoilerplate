{
  "$schema": "https://json.schemastore.org/package",
  "main": "src/index.ts",
  "version": "1.0.0",
  "scripts": {
    "git": "git add . && git commit -m \"$(date '+%-m').$(date '+%-d').$(date '+%-H')\"",
    "upd": "pnpm npm-check-updates -u",
    "dev": "pnpm nodemon",
    "serve": "pnpm ts-node src/index.ts",
    "db:view": "prisma studio",
    "db:migrate": "prisma db migrate dev",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:gen": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@trpc/server": "^10.15.0",
    "chalk": "^4.1.2",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "swagger-ui-express": "^4.6.2",
    "trpc-openapi": "^1.1.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/cors": "^2.8.13",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.0",
    "@types/swagger-ui-express": "^4.1.3",
    "nodemon": "^2.0.21",
    "npm-check-updates": "^16.7.12",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
